}
.social-links li a {
    position: relative;
}
.social-links li .facebook {
    background: url(../images/social-icon.png) no-repeat 0 0;
    height: 37px;
    width: 37px;
    display: inline-block;
}
.social-links li .facebook:after {
    border-radius: 50%;
    box-sizing: content-box;
    content: "";
    height: 100%;
    pointer-events: none;
    position: absolute;
    width: 100%;
    box-shadow: 0 0 10px 4px #6f9bc0;
    left: -3px;
    opacity: 0;
    padding: 3px;
    top: -3px;
    transform: scale(0.8);
    transition: transform 0.2s ease 0s, opacity 0.2s ease 0s;
}
.social-links li .facebook:hover:after {
    opacity: 1;
    transform: scale(1);
}
.social-links li .twitter {
    background: url(../images/social-icon.png) no-repeat -53px 0;
    height: 37px;
    width: 37px;
    display: inline-block;
}
.social-links li .twitter:after {
    border-radius: 50%;
    box-sizing: content-box;
    content: "";
    height: 100%;
    pointer-events: none;
    position: absolute;
    width: 100%;
    box-shadow: 0 0 10px 4px #6ab1cc;
    left: -3px;
    opacity: 0;
    padding: 3px;
    top: -3px;
    transform: scale(0.8);
    transition: transform 0.2s ease 0s, opacity 0.2s ease 0s;
}
.social-links li .twitter:hover:after {
    opacity: 1;
    transform: scale(1);
}
.social-links li .gPlus {
    background: url(../images/social-icon.png) no-repeat -106px 0;
    height: 37px;
    width: 37px;
    display: inline-block;
}
.social-links li .gPlus:after {
    border-radius: 50%;
    box-sizing: content-box;
    content: "";
    height: 100%;
    pointer-events: none;
    position: absolute;
    width: 100%;
    box-shadow: 0 0 10px 4px #b66363;
    left: -3px;
    opacity: 0;
    padding: 3px;
    top: -3px;
    transform: scale(0.8);
