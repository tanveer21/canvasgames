/*Science*/
.science h1 {
    font-size:30px;
    color:#0087cb;
}
.science .has-shadow {
    position:relative;
    background:none;
    margin:0 auto;
    padding:0;
}
.science .has-shadow:before {
    border:8px solid #ececec;
    border-radius: 100%;
    bottom: 0;
    box-shadow:0 0 10px -3px rgba(0, 0, 0, 0.3) inset;
    content: "";
    left: 0;
    position: absolute;
    right: 0;
    top: 0;    
}
.science p {
    font-size:15px;
    color:#616161;
}
.science .row:before {
    content:"";
    border-top: 2px dashed #c9c9c9;
    display:block;
    margin:4em auto;
    width:67%;
}
.science .thumb {
    position:relative;
    z-index:9;
}
.science .thumb:after {
    border-left: 2px dashed #ccc;
    content: "";
    display: block;
    height:100%;
    margin:0;
    position: absolute;
    left:49.5%;
    bottom:-56px;
    z-index:-1;
}
.science .thumb:before {
    border-left: 2px dashed #ccc;
    content: "";
    display: block;
    height:100%;
    margin:0;
    position: absolute;
    right:49.5%;
    top:-56px;
    z-index:-1;
}
.science .row:first-child:before {
    border:none;
    margin-top:0;
}
.science .row:first-child .thumb:before {
    display:none;
}
.science .row:last-child .thumb:after {
    display:none;
}
.chart {
  position: relative;
  width:100%;
  height:400px;  
}
.pieTip {
  position: absolute;
  float: left;
  min-width: 30px;
  max-width: 300px;
  padding: 5px 18px 6px;
  border-radius: 2px;
  background: rgba(255,255,255,.97);
  color: #444;
  font-size:14px;
  text-shadow: 0 1px 0 #fff;
  text-transform: uppercase;
  text-align: center;
  line-height: 1.3;
  letter-spacing: .06em;
  box-shadow: 0 0 3px rgba(0,0,0,0.2), 0 1px 2px rgba(0,0,0,0.5);
  -webkit-transform: all .3s;
     -moz-transform: all .3s;
      -ms-transform: all .3s;
       -o-transform: all .3s;
          transform: all .3s;
  pointer-events: none;
}
.pieTip:after {
      position: absolute;
      left: 50%;
      bottom: -6px;
      content: "";
      height: 0;
      margin: 0 0 0 -6px;
      border-right: 5px solid transparent;
      border-left: 5px solid transparent;
      border-top: 6px solid rgba(255,255,255,.95);
      line-height: 0;
}
.chart path {
    cursor: pointer;
}
.list-range {
