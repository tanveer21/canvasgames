.social-links {
    padding-left: 0;
    margin: -10px 0 0 0;
}
.social-links li {
    list-style: none;
    display: inline-block;
    margin: 0 6px;
}
.social-links li:first-child {
    margin-left: 0;
}
.social-links li:last-child {
