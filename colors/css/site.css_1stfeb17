/* tutorial button */

.control-buttons-bottom {
	text-align:center;
	position:relative;
}
.control-buttons-bottom ul {
	width:100%;
	position:absolute;
	top:-70px;
	list-style:none;
	padding:0;
	margin:0;
}
.control-buttons-bottom ul.bottom-btn-ul {
	top:-55px;
}
.control-buttons-bottom ul li {
	color:#fff;
	font-size:16px;
	display:inline-block;
	font-weight:normal;
	vertical-align:bottom;
	font-family:"poppinRegular";
	position:relative;
}
.control-buttons-bottom ul li span {
    display: inline-block;
    margin: 0 10px;
    position: absolute;
    top: 0px;
	width:100%;
	min-width:250px;
}
.control-buttons-bottom ul li a {
	display:inline-block;
}
.control-buttons-bottom ul li img {
	width:25px;
	height:auto;
	display:block;
}
.control-buttons-bottom ul li:first-child {
	margin-right:1px;
}
.control-buttons-bottom ul li:first-child span {
	right:25px;
	text-align:right;
}
.control-buttons-bottom ul li:nth-child(2) {
	margin-right:2px;
	pointer-event:none;
}
.control-buttons-bottom ul li:nth-child(2) span.text-blink {
    top: 29px;
    left: -123px;
}
.control-buttons-bottom ul li img {
	opacity:0.3;
}
.control-buttons-bottom ul li a.active img {
	opacity:1;
}
.control-buttons-bottom ul li:last-child {
}
.control-buttons-bottom ul li:last-child span {
	left:25px;
	text-align:left;
}
.control-buttons-bottom ul li:nth-child(2) img {
	width:24px;
}
.control-buttons-bottom ul li:nth-child(2) .up-arrow-disable {
	position:absolute;
	top:-32px;
}
.text-blink {
    color: #fff;
    -webkit-animation-name: blink; /* Chrome, Safari, Opera */
    -webkit-animation-duration: 0.6s; /* Chrome, Safari, Opera */
	-webkit-animation-iteration-count: infinite; /* Chrome, Safari, Opera */
	-webkit-animation-timing-function: ease-in-out;
    animation-name: blink;
    animation-duration: 0.6s;
	animation-timing-function: ease-in-out;
	animation-iteration-count: infinite;
}
/* Chrome, Safari, Opera */
@-webkit-keyframes blink {
    0%   {opacity: 1;}
    50% {opacity: 0.2;}
    100% {opacity: 1;}
}

/* Standard syntax */
@keyframes blink {
    0%   {opacity: 1;}
    50% {opacity: 0.2;}
    100% {opacity: 1;}
}

