    text-rendering: optimizeLegibility;
}
.game-list figure {
    display:block;
    position:relative;
    margin-bottom:10px;
}
.game-list figure:before {
    content: "";
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	box-shadow: inset 0 0 30px -5px rgba(0,0,0,.6);
	-moz-box-shadow: inset 0 0 30px -5px rgba(0,0,0,.6);
	-webkit-box-shadow: inset 0 0 30px -5px rgba(0,0,0,.6);
}
.game-list figure img {
    width:100%;
}
.game-list .lock figure:after {
    content:"";
    background: url('data:image/png;base64,') no-repeat;
    background-size:45%;
    background-position:12px 8px;
    background-color: rgba(0, 0, 0, 0.5);
    width:44px;
    height:44px;
    display:inline-block;
    position:absolute;
    left:0;
    bottom:0;
}
.game-list .lock:hover:before {
    background-color:rgba(0, 0, 0, 0.3);    
    border:1px solid #b0b0b0;
    content: "";
    height: 125%;
    left: 0;
    margin:-45px -10px 0 -10px;
    position: absolute;
    top: 0;
    width: 108%;
    z-index:9;
}
.game-list .lock .unlock {
    height: 100%;
    left: 0;
    position: absolute;
    text-align: center;
    top:-1.2em;
    width: 100%;
    z-index:99;
    display:none;    
}
.game-list .unlock p {
    font-size:16px;
    font-family: 'HelveticaMd';
    text-rendering: optimizeLegibility;
    padding: 0 15px;
    margin-top:10px;
    margin-bottom:15px;
    text-shadow: 1px 1px 0 #fff;
}
.game-list .unlock .btn-lock .icon-lock {
    background-size:70%;
    margin:0px 5px -12px 0;
}
.game-list .lock:hover figure:after {
    display:none;
}
.game-list .lock:hover .unlock {
    display:block;
}
.ribbon {
  position: absolute;
  left: -5px; top: -5px;
  z-index: 1;
  overflow: hidden;
  width: 75px; height: 75px;
  text-align: right;
}
.ribbon span {
  font-family: 'HelveticaBd';
  text-rendering: optimizeLegibility;
  font-size: 14px;
  font-weight: bold;
  color: #FFF;
  text-transform: uppercase;
  text-align: center;
  line-height: 24px;
  transform: rotate(-45deg);
  -webkit-transform: rotate(-45deg);
  width: 100px;
  display: block;
  background: #79A70A;
  background: linear-gradient(#03d538 0%, #03BB31 100%);
  box-shadow: 0 3px 10px -5px rgba(0, 0, 0, 1);
  position: absolute;
  top: 17px;
  left: -21px;
}
.ribbon span::before {
  content: "";
  position: absolute; left: 0px; top: 100%;
  z-index: -1;
  border-left: 3px solid #03BB31;
  border-right: 3px solid transparent;
  border-bottom: 3px solid transparent;
  border-top: 3px solid #03BB31;
}
.ribbon span::after {
  content: "";
  position: absolute; right: 0px; top: 100%;
  z-index: -1;
  border-left: 3px solid transparent;
  border-right: 3px solid #03BB31;
  border-bottom: 3px solid transparent;
  border-top: 3px solid #03BB31;
}
