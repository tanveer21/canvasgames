}
.readmore {
    max-height:100px;
    transition: max-height 0.15s ease-out;
    overflow: hidden;
}
.readmore.in {
    max-height:500px;
}
.morelink {
    display: block;
    background:#4677bd;
    padding:6px 15px;
    color:#fff;
    text-decoration:none;
    clear:both;
    max-width:155px;
    border-radius:20px;
    margin-top:2em;
    position:relative;
    font-family: "HelveticaMd";
    font-size:13px;
}
.morelink:hover, .morelink:focus {
    color:#fff;
    text-decoration:none;
}
.morelink:after {
    content:"";
    background: url('data:image/png;base64,') no-repeat;
    height: 6px;
    position: absolute;
    right: 12px;
    top: 12px;
    width: 10px;
    transition:all 0.1s ease-in-out 0.1s;
}
.morelink.less {
    background:#0e91a1;
}
.morelink.less:after {    
    -webkit-transform: rotate(180deg);
    -moz-transform: rotate(180deg);
    -ms-transform: rotate(180deg);
    -o-transform: rotate(180deg);
    transform: rotate(180deg);
}
