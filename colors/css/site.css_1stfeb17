.chart-toggle {
    display:table;
    margin:1em auto auto auto;
}
.chart-toggle span {
    display:block;
}
.chart-toggle li {
    background:#d7d7d7;
    border-radius:5px;
    position: relative;
    padding:6px;
    line-height:10px;
    width:38px;
    height:38px;
    vertical-align:bottom;    
    margin:0 5px;
}
.chart-toggle li.active {    
	background:#ff4e02;    
}
.chart-toggle li.active:after {
	bottom: 100%;
	left: 50%;
	border: solid transparent;
    border-color:rgba(255, 78, 2, 0);
	border-bottom-color: #ff4e02;
	border-width:6px;	
    border-bottom-width: 8px;
	content: " ";
	height: 0;
	width: 0;
    margin-left:-6px;
	position: absolute;
	pointer-events: none;
}
