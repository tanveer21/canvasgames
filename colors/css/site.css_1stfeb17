/*Lecture page*/
.lecture .media {
    border-top:1px solid #ebebeb;
    margin-top: 3em;
    padding-top: 3em;
}
.lecture .media:first-child {
    border-top:none;
    margin-top:15px;
    padding-top:0;
}
.lecture .media-left {
    padding-right: 30px;
    transition:all 0.1s ease-in-out 0.1s;
}
.lecture .media-heading {
    margin-bottom:1.5em;
    transition:all 0.1s ease-in-out 0.1s;
}
.lecture .media-object {
    border:5px solid #efefef;
}
.lecture h5 {
    font-size:15px;
    color:#3c3c3c;
    transition:all 0.1s ease-in-out 0.1s;
}
.lecture, .lecture li, .lecture p {    
